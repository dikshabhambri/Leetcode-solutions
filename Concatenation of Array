import java.util.*;
class Solution {
    
    public int[] getConcatenation(int[] nums) {
      int ans[] = new int [nums.length*2];
      Scanner sc = new Scanner (System.in);

      
      for ( int i = 0; i<nums.length*2; i++){
          if (i<nums.length){
              ans [i] = nums[i];
          }
          else{
              ans [i] = nums[i-nums.length];
          }
      }
      return ans;
    }
}
